{
  "name": "ng-slider",
  "version": "2.3.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production ng-slider",
    "postbuild": "cp ./README.md ./dist/@jaspero/ng-slider && cp LICENSE ./dist/@jaspero/ng-slider",
    "test": "ls",
    "test:app": "ls",
    "lint": "ng lint ng-slider",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index -g"
    }
  },
  "lint-staged": {
    "*.{ts,json,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "14.2.8",
    "@angular/common": "14.2.8",
    "@angular/compiler": "14.2.8",
    "@angular/core": "14.2.8",
    "@angular/forms": "14.2.8",
    "@angular/http": "7.2.15",
    "@angular/platform-browser": "14.2.8",
    "@angular/platform-browser-dynamic": "14.2.8",
    "@angular/router": "14.2.8",
    "@jaspero/ng-slider": "2.3.2",
    "core-js": "3.1.4",
    "hammerjs": "2.0.8",
    "rxjs": "6.6.7",
    "tslib": "2.0.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.7",
    "@angular-devkit/build-ng-packagr": "0.1002.4",
    "@angular/cli": "14.2.7",
    "@angular/compiler-cli": "14.2.8",
    "@angular/language-service": "14.2.8",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.0.6",
    "@types/jasmine": "3.3.13",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "12.11.1",
    "codelyzer": "5.1.2",
    "husky": "3.0.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.2",
    "karma-jasmine": "4.0.0",
    "karma-jasmine-html-reporter": "1.5.0",
    "lint-staged": "9.0.2",
    "ng-packagr": "14.2.2",
    "prettier": "1.15.3",
    "protractor": "7.0.0",
    "semantic-release": "19.0.3",
    "ts-node": "8.3.0",
    "tslint": "6.1.0",
    "typescript": "4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jaspero/ng-slider.git"
  },
  "release": {
    "pkgRoot": "dist/@jaspero/ng-slider",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
